{
  "name": "rdfjs-c14n-tester — Deno version",
  "version": "1.0.0",
  "description": "Official testing the rdfjs-c14n package as an external dependency — deno version",
  "main": "main.ts",
  "author": "Ivan Herman <ivan@w3.org> (https://www.w3.org/People/Ivan/)",
  "license": "W3C-20150513",
  "tasks": {
    "run": "deno run --allow-net --allow-read --allow-write main.ts",
    "compile": "deno compile -o c14n-tester --allow-net --allow-read --allow-write main.ts",
    "docs": "deno doc --html --name=\"rdfjs-c14n tester\" --private --no-remote --output=./docs ./main.ts ./lib/batch.ts ./lib/earl.ts ./lib/rdfn3.ts ./lib/types.ts ./lib/utils.ts"
  },
  "imports": {
    "commander":               "npm:commander",
    "n3":                      "npm:n3",
    "@rdfjs/types":            "npm:@rdfjs/types",
    "underscore":              "npm:underscore",
    "./lib/utils":             "./lib/utils.ts",
    "./lib/types":             "./lib/types.ts",
    "./lib/earl":              "./lib/earl.ts",
    "./lib/batch":             "./lib/batch.ts",
    "./lib/rdfn3":             "./lib/rdfn3.ts",
    "./utils":                 "./utils.ts",
    "./types":                 "./types.ts",
    "./earl":                  "./earl.ts",
    "./batch":                 "./batch.ts",
    "./rdfn3":                 "./rdfn3.ts",
    "rdfjs-c14n":              "npm:rdfjs-c14n@^3.1.0",
    "event-emitter-promisify": "npm:event-emitter-promisify"
  }
}
